<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Product.
    -->
    <changeSet id="20190819185022-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="house_farm_id"
                                 baseTableName="product"
                                 constraintName="fk_product_house_farm_id"
                                 referencedColumnNames="id"
                                 referencedTableName="house_farm"/>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="product"
                                 constraintName="fk_product_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>

    </changeSet>
</databaseChangeLog>
